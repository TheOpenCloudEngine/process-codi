<% if(value){ %>
	<div class="depth1">
        <div class="fist_menu">
            <ul><li>
                    <a href="#">
                        <span class="icon-feed left_menu_icon"></span>
                        <span style="padding:0px;margin:0px;width:auto;" class="left_menu_text"><%=fields.label.here()%></span>
                    </a>
                    <% if(!mw3.isHiddenMethod(methods.expand)){ %>
                    <span class="west_more_btn">
                        <a href="javascript:<%=methods.expand.caller()%>">더보기</a>
                    </span>
                    <% } %>
                    <% if(!mw3.isHiddenMethod(methods.collapse)){ %>
                    <span class="west_more_btn">
                        <a href="javascript:<%=methods.collapse.caller()%>">접기</a>
                    </span>
                    <% } %>
            </li></ul>
        </div>
	</div>

	<div class="depth2">
		<div class="fist_menu">		
			<%=fields.child.here()%>
			<ul>
				<li>
					<a href="#" onClick="<%=methods.popupAdd.caller()%>" class="add_new_label">
                        <span class="glyphicon glyphicon-plus-sign left_menu_icon"  style="top:3px"></span>
                        새<%=mw3.localize(value.label)%>
                    </a>
					<div class="clear"></div>
				</li>
			</ul>
		</div>
	</div>
<%
	}
%>
