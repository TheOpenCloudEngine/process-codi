<!--replace-->

		<%
			if(value){

			var isChat = (value.defVerId == null && value.secuopt == '1');



			var isProcess = (value.defVerId != null);
			var isEssenceProcess = isProcess && value.defVerId.indexOf(".method") > 1;

			if(isEssenceProcess){
		%>

		<div class="col-sm-6 col-md-4" id="objDiv_<%=objectId%>">
			<div class="thumbnail">
				<%
					var zoom = "0.4";
					if(value.preview){

				%>

				<div style="zoom: <%=zoom%>; -ms-zoom: <%=zoom%>; -webkit-zoom: <%=zoom%>; -moz-transform:  scale(<%=zoom%>,<%=zoom%>); -moz-transform-origin: left center;">
					<%=fields.preview.here()%>
				</div>

				<%}%>

				<div class="caption">
					<h3><a href="#" onclick="<%=methods.detailInTwoColumn.caller()%>"><%=value.name%></a> <%=fields.instanceDrag.here()%></h3>
					<p>
						Started at <%=fields.startedDate.here({when: 'timeago'})%> by <%=fields.initiator.here({when: 'timeago'}) %>

						<% if(value.currentUser && value.currentUser.userId && (value.initiator.userId!=value.currentUser.userId || value.name!=value.lastCmnt)){%>
						<img src="portrait/<%=value.currentUser.userId%>.thumnail.jpg" width="20px" align=left onerror="$(this).remove();">
						<%=value.lastCmnt%>
						<%}%>


						<%if(value.status=='Completed'){%> ( <%=mw3.localize('$Complete')%> ) <%}%>
					</p>

				</div>
			</div>
		</div>


<%}

}%>

